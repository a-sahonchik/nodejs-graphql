{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "indent": "off",
    "max-len": ["error", 120],
    "@typescript-eslint/indent": ["error", 2],
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "import/prefer-default-export": "off",
    "object-curly-newline": "off"
  }
}
